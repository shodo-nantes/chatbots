{
    "name": "birthday-bot",
    "version": "1.0.0",
    "description": "A awesome google chatbot birthday",
    "scripts": {
        "prepare": "husky install",
        "lint": "eslint bots --ext .ts,.tsx,.js,.jsx",
        "lint:fix": "npm run lint -- --fix",
        "lint-staged": "eslint $(git diff --cached --name-only --diff-filter=ACMRTUXB | grep -E '\\.(ts|tsx|js|jsx)$') --ext .ts,.tsx,.js,.jsx",
        "lint-staged:fix": "npm run lint-staged -- --fix",
        "test": "jest"
    },
    "dependencies": {
        "luxon": "^3.3.0"
    },
    "devDependencies": {
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-github": "^4.7.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-sonarjs": "^0.19.0",
        "eslint-plugin-unicorn": "^46.0.0",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "prettier": "^2.8.7"
    },
    "license": "MIT"
}
